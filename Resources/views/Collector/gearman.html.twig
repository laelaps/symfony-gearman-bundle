{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    {# the web debug toolbar content #}
    {% set icon %}
        <img alt="Gearman" src="data:image/png;base64," />
        <span class="sf-toolbar-status">{{ collector.requestcount }}</span>
    {% endset %}
    {% set text %}
        <div class="sf-toolbar-info-piece">
            <b>Gearman requests</b>
            <span>{{ collector.requestcount }}</span>
        </div>
        <div class="sf-toolbar-info-piece">
            <b>Response time</b>
            <span>{{ '%0.2f'|format(collector.time * 1000) }} ms</span>
        </div>
{% endset %}
    {% include 'WebProfilerBundle:Profiler:toolbar_item.html.twig' with { 'link': profiler_url } %}
{% endblock %}

{% block menu %}
    {# the menu content #}
    <span class="label">
        <span class="icon"><img src="{{ asset('bundles/laelapsgearman/images/gearman.png') }}" alt="" /></span>
        <strong>Gearman</strong>
        <span class="count">
            <span>{{ collector.requestcount }}</span>
            <span>{{ '%0.0f'|format(collector.time * 1000) }} ms</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
  <h2>Gearman</h2>
  <h3>Statistics</h3>
  <table>
    <thead><tr>
      <th>Worker</th>
      <th>Priority</th>
      <th>Time (ms)</th>
      <th>Response</th>
    </tr></thead>
    <tbody><tr>
    {% for call in collector.calls %}
      <td>{{ call.workload[0] }}</td>
      <td>{{ call.name }}</td>
      <td>{{ '%0.2f'|format(call.time * 1000) }}</td>
      <td>{{ call.result }}</td>
    {% endfor %}
    </tr></tbody>
  </table>
{% endblock %}